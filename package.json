{
  "name": "@mejiaj/portfolio",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "preview": "astro preview",
    "start": "astro dev",
    "lint": "npm run lint:sass && npm run lint:js",
    "lint:js": "npx eslint './src/**/*.js'",
    "lint:sass": "npx stylelint '**/*.scss'",
    "lint:fix:sass": "npx stylelint --fix '**/*.{sass,scss}'",
    "lint:fix:js": "npx eslint --fix './src/**/*.js'",
    "prettier": "npx prettier --check '*.{md,mdx}' './src/**/*.{css,scss,sass,js}'",
    "prettier:fix:js": "npx prettier --write './src/**/*.js'",
    "prettier:fix:sass": "npx prettier --write '**/*.{sass,scss}'",
    "prettier:fix:md": "npx prettier --write '**/*.{md,mdx}'",
    "prettier:fix": "npx prettier --write '**/*.{sass,scss,js,md}'",
    "test:a11y": "npx pa11y-ci --sitemap http://localhost:4321/sitemap-index.xml --sitemap-find https://www.jmejia.com --sitemap-replace http://localhost:4321",
    "build:sass": "sass './src/scss/styles.scss' './public/styles.css'",
    "build": "astro build"
  },
  "devDependencies": {
    "@18f/identity-stylelint-config": "4.1.0",
    "@astrojs/sitemap": "3.5.1",
    "@eslint/js": "9.34.0",
    "astro": "5.13.5",
    "eslint": "9.34.0",
    "eslint-config-prettier": "10.1.8",
    "globals": "16.3.0",
    "pa11y-ci": "4.0.1",
    "prettier": "3.6.2",
    "sass": "1.91.0",
    "stylelint": "16.23.1"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "typescript": "5.9.2"
  }
}
