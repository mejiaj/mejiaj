{
  "name": "@mejiaj/portfolio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "lint": "npm run lint:sass & npm run lint:js",
    "lint:js": "npx eslint './src/**/*.js'",
    "lint:sass": "npx stylelint '**/*.scss'",
    "lint:fix:sass": "npx stylelint --fix '**/*.{sass,scss}'",
    "lint:fix:js": "npx eslint --fix './src/**/*.js'",
    "prettier": "npx prettier --check './src/**/*.{css,scss,sass,js}'",
    "prettier:fix:js": "npx prettier --write './src/**/*.js'",
    "prettier:fix:sass": "npx prettier --write '**/*.{sass,scss}'",
    "prettier:fix": "npx prettier --write '**/*.{sass,scss,js}'",
    "build:sass": "sass './src/scss/styles.scss' './public/styles.css'",
    "build": "astro build"
  },
  "devDependencies": {
    "@18f/identity-stylelint-config": "^4.1.0",
    "@astrojs/sitemap": "^3.2.1",
    "@eslint/js": "^9.21.0",
    "astro": "^5.4.1",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "globals": "^16.0.0",
    "prettier": "^3.5.2",
    "sass": "^1.85.1",
    "stylelint": "^16.15.0"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "scroll-out": "^2.2.12",
    "typescript": "^5.8.2"
  }
}
