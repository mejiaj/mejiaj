{
  "name": "@mejiaj/portfolio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "lint": "npm run lint:sass & npm run lint:js",
    "lint:js": "npx eslint './src/**/*.js'",
    "lint:sass": "npx stylelint '**/*.scss'",
    "lint:fix:sass": "npx stylelint --fix '**/*.{sass,scss}'",
    "lint:fix:js": "npx eslint --fix './src/**/*.js'",
    "prettier": "npx prettier --check './src/**/*.{css,scss,sass,js}'",
    "prettier:fix:js": "npx prettier --write './src/**/*.js'",
    "prettier:fix:sass": "npx prettier --write '**/*.{sass,scss}'",
    "build:sass": "sass './src/scss/styles.scss' './public/styles.css'",
    "build": "astro build"
  },
  "devDependencies": {
    "@18f/identity-stylelint-config": "^2.0.0",
    "@astrojs/sitemap": "^1.3.1",
    "astro": "^2.3.0",
    "eslint": "^8.40.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^2.8.8",
    "sass": "^1.62.0",
    "scroll-out": "^2.2.12",
    "stylelint": "^15.10.1"
  },
  "dependencies": {
    "balance-text": "^3.3.1",
    "scroll-out": "^2.2.12"
  }
}
